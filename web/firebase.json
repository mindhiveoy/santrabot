{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/graphql/**",
        "function": "app"
      },
      {
        "source": "/csv/**",
        "function": "app"
      },
      {
        "source": "/theme/**",
        "function": "app"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
     
    ]
  }
}
