{
  "name": "seoppi-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf -f -r -d dist",
    "start": "cross-env NODE_ENV=development && webpack-dev-server --env.NODE_ENV=development --mode=development --config=webpack/webpack.config.development.js",
    "start:staging": "cross-env NODE_ENV=staging && webpack-dev-server --mode=production --env.NODE_ENV=staging --config=webpack/webpack.config.development.js",
    "start;production": "cross-env NODE_ENV=production && webpack-dev-server --mode=production --env.NODE_ENV=production --config=webpack/webpack.config.development.js",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "tslint --fix -c tslint.json ./src/**/*.ts* -p ./tsconfig.json",
    "lint:css": "stylelint --fix './src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "predeploy:development": "yarn build:development",
    "deploy:development": "firebase use default && firebase deploy --only hosting",
    "predeploy:staging": "yarn build:staging",
    "deploy:staging": "firebase use staging && firebase deploy --only hosting",
    "predeploy:production": "yarn build:production",
    "deploy:production": "firebase use production && firebase deploy --only hosting",
    "build:stats": "cross-env NODE_ENV=production && webpack --env.NODE_ENV=production --config=webpack/webpack.config.production.js --json > stats.json",
    "build:stats-dev": "cross-env NODE_ENV=production && webpack --env.NODE_ENV=production --config=webpack/webpack.config.development.js --json > stats.json",
    "prebuild:production": "yarn clean",
    "prebuild:staging": "yarn clean",
    "prebuild:development": "yarn clean",
    "build:production": "cross-env NODE_ENV=production && webpack --env.NODE_ENV=production --config=webpack/webpack.config.production.js --progress",
    "build:staging": "cross-env NODE_ENV=staging && webpack --mode=production --env.NODE_ENV=staging --config=webpack/webpack.config.production.js",
    "build:development": "cross-env NODE_ENV=default && webpack --mode=production --env.NODE_ENV=development --config=webpack/webpack.config.production.js",
    "reset-modules": "rimraf ./node_modules/ && rimraf yarn.lock && yarn"
  },
